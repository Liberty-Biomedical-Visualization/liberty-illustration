{
  "name": "liberty-illustration",
  "description": "Official website of Liberty Biomedical Visualization",
  "version": "0.1.0",
  "author": "Nathaniel J. Liberty <nathaniel.liberty@gmail.com> (https://nathanieljliberty.com)",
  "homepage": "https://github.com/Liberty-Biomedical-Visualization/liberty-illustration",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write",
    "format:all": "npm run format -- .",
    "format:check": "prettier --check",
    "format:check:all": "npm run format:check -- .",
    "lint": "next lint --max-warnings 0",
    "lint:all": "npm run lint -- --dir .",
    "prepare": "husky",
    "start": "next start",
    "test": "jest",
    "test:e2e": "playwright",
    "test:e2e:codegen": "npm run test:e2e -- codegen",
    "test:e2e:install": "npm run test:e2e -- install --with-deps",
    "test:e2e:run": "npm run test:e2e -- test --pass-with-no-tests",
    "test:e2e:open": "npm run test:e2e -- test --ui",
    "test:integration": "jest --projects jest.integration.config.ts",
    "test:unit": "jest --projects jest.component.config.ts jest.unit.config.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.47.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
